{
  "name": "@yvog/blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm run build:sitemap && next export",
    "build:sitemap": "next-sitemap --config sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "npx prettier --write \"{pages,components}/**/*.{ts,tsx,js,json}\" "
  },
  "engines": {
    "node": ">=16.14.0 <17"
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mapbox/rehype-prism": "^0.8.0",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.4.3",
    "next": "12.1.0",
    "next-mdx-remote": "^4.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remark-gfm": "^3.0.1",
    "tss-react": "^3.4.2"
  },
  "devDependencies": {
    "@types/node": "17.0.18",
    "@types/react": "17.0.39",
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-mui-path-imports": "^0.0.15",
    "next-sitemap": "^1.5.18",
    "prettier": "2.5.1",
    "typescript": "4.5.5"
